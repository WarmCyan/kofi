#!/bin/bash

cache_dir="./testcache/"

name=$(datetimestamp)
desc=$1
tags=$2

cp template $cache_dir/$name
set-date "created" $cache_dir/$name
set-date "updated" $cache_dir/$name
#pushd $cache_dir > /dev/null
#nvim "+normal 4GA" $name
#popd > /dev/null
echo $name
